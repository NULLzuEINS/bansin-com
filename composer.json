{
    "name": "basincom",
    "require": {
        "php": "^5.4 || ^7.0",
        "mnsami/composer-custom-directory-installer": "1.1.*",
        "getkirby/kirby": "2.5.8",
        "getkirby/toolkit": "2.5.8",
        "getkirby/panel": "2.5.8"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "getkirby/kirby",
                "version": "2.5.8",
                "source": {
                    "type": "git",
                    "url": "https://github.com/getkirby/kirby.git",
                    "reference": "2.5.8"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "getkirby/toolkit",
                "version": "2.5.8",
                "source": {
                    "type": "git",
                    "url": "https://github.com/getkirby/toolkit.git",
                    "reference": "2.5.8"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "getkirby/panel",
                "version": "2.5.8",
                "source": {
                    "type": "git",
                    "url": "https://github.com/getkirby/panel.git",
                    "reference": "2.5.8"
                }
            }
        }
    ],
    "extra": {
        "installer-paths": {
            "./kirby": ["getkirby/kirby"],
            "./kirby/toolkit": ["getkirby/toolkit"],
            "./panel": ["getkirby/panel"]
        }
    }
}
